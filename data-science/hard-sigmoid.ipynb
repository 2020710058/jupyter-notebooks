   ],
   "source": [
    "x = np.arange(-10, 10, 0.1)\n",
    "\n",
    "plt.plot(x, hard_sigmoid(x))\n",
    "plt.plot(x, sigmoid(x))\n",
    "plt.show()"
   ]
  }
